{
  "editor.useTabStops": true,
  "editor.rulers": [125],
  "editor.wrappingStrategy": "advanced",
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 125,
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit",
      "source.fixAll": "explicit"
    },
    "editor.defaultFormatter": "trunk.io",
    "editor.formatOnSave": true
  },
  "python.defaultInterpreterPath": "${workspaceFolder}/.conda/bin/python",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true,
    ".gitattributes": true,
    "_CODER.coder.yaml": true,
    "pytest.ini": true,
    ".coverage": true,
    ".cursorrules": true,
    "_test_project.yaml": true,
    "code_helpers.egg-info": true,
    ".pytest_cache": true,
    ".logfire": true,
    ".github": true,
    "__pycache__": true,
    "alfie/__pycache__": true,
    "core/__pycache__": true,
    "network/__pycache__": true,
    "notion/__pycache__": true,
    "projects/__pycache__": true,
    "system/__pycache__": true,
    ".env.sample": true,
    "notion/management/__pycache__": true,
    "**/__pycache__": true,
    "**/__init__.py": true
  },
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.folders.associations": {
    "notion": "notion",
    "nginxproxy": "nginxproxy",
    "management": "src",
    "cloudflare": "cloudflareicon",
    "agents": "godot",
    "vault": "vault"
  },
  "explorer.autoReveal": "focusNoScroll",
  "explorer.compactFolders": true,
  "files.watcherExclude": {
    "**/__pycache__": true
  },
  "hide-files.files": [],
  "codeseek.apiKey": "sk-77efc16cbd2d414ba3859358d7da0675"
}
