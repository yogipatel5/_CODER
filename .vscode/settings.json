{
  "editor.defaultFormatter": null,
  "editor.useTabStops": true,
  "editor.rulers": [80, 100],

  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "python.defaultInterpreterPath": "${workspaceFolder}/.conda/bin/python",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true,
    ".gitattributes": true,
    "_CODER.coder.yaml": true,
    "pytest.ini": true,
    "pyproject.toml": true,
    ".coverage": true,
    ".cursorrules": true,
    "_test_project.yaml": true,
    "code_helpers.egg-info": true,
    ".pytest_cache": true,
    ".logfire": true,
    ".github": true,
    "__pycache__": true,
    "alfie/__pycache__": true,
    "core/__pycache__": true,
    "network/__pycache__": true,
    "notion/__pycache__": true,
    "projects/__pycache__": true,
    "system/__pycache__": true,
    ".env.sample": true,
    "notion/management/__pycache__": true
  },
  "hide-files.files": []
}
