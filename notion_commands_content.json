          {
            "type": "text",
            "text": {
              "content": "# Search pages by query\npython manage.py notion search_pages \"search query\"\n\n# Search with limit\npython manage.py notion search_pages \"search query\" --limit 5"
            }
          }
        ],
        "language": "bash"
      }
