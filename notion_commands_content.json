    {
      "object": "block",
      "type": "heading_3",
      "heading_3": {
        "rich_text": [
          {
            "type": "text",
            "text": {
              "content": "List Pages"
            }
          }
        ]
      }
    },
    {
      "object": "block",
      "type": "code",
      "code": {
        "rich_text": [
          {
            "type": "text",
            "text": {
              "content": "# List all pages\npython manage.py notion list_pages\n\n# List pages with limit\npython manage.py notion list_pages --limit 10\n\n# List pages from specific database\npython manage.py notion list_pages --database <database_id>"
            }
          }
        ],
        "language": "bash"
      }
    },
    {
      "object": "block",
      "type": "heading_3",
      "heading_3": {
        "rich_text": [
          {
            "type": "text",
            "text": {
              "content": "Create Page"
            }
          }
        ]
      }
    },
    {
      "object": "block",
      "type": "code",
      "code": {
        "rich_text": [
          {
            "type": "text",
            "text": {
              "content": "# Create a page under another page\npython manage.py notion create_page <parent_page_id> \"Page Title\"\n\n# Create a page with properties\npython manage.py notion create_page <parent_page_id> \"Page Title\" --properties key1=value1 key2=value2"
            }
          }
        ],
        "language": "bash"
      }
    },
    {
      "object": "block",
      "type": "heading_3",
      "heading_3": {
        "rich_text": [
          {
            "type": "text",
            "text": {
              "content": "Get Page"
            }
          }
        ]
      }
    },
    {
      "object": "block",
      "type": "code",
      "code": {
        "rich_text": [
          {
            "type": "text",
            "text": {
              "content": "# Get basic page info\npython manage.py notion get_page <page_id>\n\n# Get page with content\npython manage.py notion get_page <page_id> --include-content\n\n# Get raw API response\npython manage.py notion get_page <page_id> --raw"
            }
          }
        ],
        "language": "bash"
      }
    },
    {
      "object": "block",
      "type": "heading_3",
      "heading_3": {
        "rich_text": [
          {
            "type": "text",
            "text": {
              "content": "Update Page"
            }
          }
        ]
      }
    },
    {
      "object": "block",
      "type": "code",
      "code": {
        "rich_text": [
          {
            "type": "text",
            "text": {
              "content": "# Update page properties\npython manage.py notion update_page <page_id> --properties title=\"New Title\" status=\"In Progress\"\n\n# Update page with JSON\npython manage.py notion update_page_json <page_id> --json-file update.json"
            }
          }
        ],
        "language": "bash"
      }
    },
    {
      "object": "block",
      "type": "heading_3",
      "heading_3": {
        "rich_text": [
          {
            "type": "text",
            "text": {
              "content": "Delete Page"
            }
          }
        ]
      }
    },
    {
      "object": "block",
      "type": "code",
      "code": {
        "rich_text": [
          {
            "type": "text",
            "text": {
              "content": "# Delete (archive) a page\npython manage.py notion delete_page <page_id>"
            }
          }
        ],
        "language": "bash"
      }
    },
    {
      "object": "block",
